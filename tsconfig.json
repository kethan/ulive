{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    // "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "noImplicitAny": false
  },
  "include": [
    "src/*.ts",
    "src/*.d.ts"
  ]
}